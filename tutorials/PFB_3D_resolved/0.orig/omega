FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.25;

boundaryField
{

    "(outlet|inlet|atmosphere)"
    {
        type            inletOutlet;
        inletValue      uniform 0.25;
        value           uniform 0.25;
    }

    "(stationaryWalls|floater|BB1|BB2|BB3|frontAndback)"
    {
        type            omegaWallFunction;
        value           uniform 0.25;
    }

}


