porousforces
{
    type                porousforces;
    //functionObjectLibs  
    libs ("libporousforce.so");
    //outputControl       timeStep;
    //outputInterval      1;
     
    patches             ("porous" );
    //regionType  cellZone;
    //name        porous;
    
    pName               p;
    Uname               U;
    rho             rhoInf;
    log                 true;
     
    CofR                (-0.2 0 0.95);
     
    rhoInf              1000;
	porosity true;
}
 
